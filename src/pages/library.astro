---
import BaseHead from "../components/BaseHead.astro";
import Header from "../components/Header.astro";
import Footer from "../components/Footer.astro";

import { getLinks } from "../scripts/notion.js";
const links = await getLinks();

import FormattedDate from "../components/FormattedDate.astro";
---

<style>
  header {
    padding: 0 1rem;
  }
  .notes {
    display: block;
    margin-top: 0.5rem;
  }
  .meta {
    color: #888;
    display: flex;
    font-size: 0.75rem;
    margin-top: 0.75rem;
  }
  .library-links {
    display: grid;
    flex-wrap: wrap;
    list-style: none;
    padding: 0 1rem;
  }

  .library-links li {
    border-top: 0.125rem solid var(--color-accent);
    margin-top: 1rem;
    padding: 1.5rem;
  }

  .link {
    word-break: break-word;
  }

  @media screen and (min-width: 768px) {
    .library-links {
      grid-template-columns: 1fr 1fr;
      grid-template-rows: min-content;
      justify-items: start;
    }
  }
  @media screen and (min-width: 1000px) {
    .library-links {
      gap: 0 2rem;
      grid-template-columns: 1fr 1fr 1fr;
    }
  }
</style>

<html lang="en">
  <head>
    <BaseHead
      title="Library | Dan Denney"
      description="Links of things I've read and watched"
    />
  </head>
  <body class="bg-white dark:bg-slate-900">
    <main
      class="bg-white relative shadow z-10 dark:bg-slate-900 dark:text-slate-200"
    >
      <Header />
      <section class="px-6 lg:px-10">
        <h1
          class="mt-4 text-3xl font-bold tracking-tight text-gray-900 sm:text-4xl dark:text-slate-100"
        >
          Library
        </h1>
        <p class="mt-2">Links of things I've read and watched.</p>
      </section>
      <ul class="library-links has-links">
        {
          links.map((link) => (
            <li>
              <a class="link" href={link.url}>
                {link.title}
              </a>
              {link.notes && <span class="notes">{link.notes}</span>}
              <span class="meta">
                {link.tag}&nbsp;
                <FormattedDate date={link.created} />
              </span>
            </li>
          ))
        }
      </ul>
